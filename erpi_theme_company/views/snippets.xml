<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="snippet_catp_comp_test" name="Test Snippet">
        <section class="snippet_catp_comp_test">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12">
                        <h1>Test Snippet</h1>
                    </div>
                </div>
            </div>
        </section>
    </template>

    <template id="snippet_catp_comp_test_icon" inherit_id="website.snippets" name="Place into bar">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="erpi_theme_company.snippet_catp_comp_test"
            t-thumbnail="/erpi_theme_company/static/img/nc-thumb.png"/>
        </xpath>
    </template>

